<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal"

    android:background="#000000"
    android:layout_height="match_parent"
    tools:context=".ticket_history">
    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_gravity="center_horizontal"
        android:contentDescription="@string/todo"
        android:layout_marginTop="50dp"
        android:src="@mipmap/finna"
        android:tint="@color/orange_400" />

    <HorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:gravity="center_horizontal"
        android:background="#000000"
        android:layout_height="wrap_content"
        tools:context=".ticket_history">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:gravity="center_horizontal"
        android:background="#000000"
        android:layout_height="wrap_content"
        tools:context=".ticket_history">

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_marginTop="40dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:textColor="@color/orange_400"
            android:layout_marginTop="40dp"
            android:layout_weight=".9"
            android:textStyle="bold"
            android:text="Ticket id"
            android:textSize="20dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/orange_400"
            android:text="Issue."
            android:layout_weight=".9"
            android:textSize="20dp"
            android:layout_marginLeft="30dp"
            android:textStyle="bold"
            android:layout_marginTop="40dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Discription"
            android:textColor="@color/orange_400"
            android:layout_weight=".9"
            android:textSize="20dp"
            android:textStyle="bold"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            ></TextView>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:textColor="@color/orange_400"
            android:layout_marginTop="40dp"
            android:layout_weight=".9"
            android:textStyle="bold"
            android:text="Request date"
            android:textSize="20dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/orange_400"
            android:text="Status"
            android:layout_weight=".9"
            android:textSize="20dp"
            android:layout_marginLeft="30dp"
            android:textStyle="bold"
            android:layout_marginTop="40dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Remarks"
            android:textColor="@color/orange_400"
            android:layout_weight=".9"
            android:textSize="20dp"
            android:textStyle="bold"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            ></TextView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            android:layout_weight=".9"
            android:text="12345"
            android:textColor="#ffffff"
            android:textSize="20dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="abcdd"
            android:layout_weight=".9"
            android:textSize="20dp"
            android:textColor="#ffffff"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="sssssidhid wed"
            android:layout_weight=".9"
            android:textSize="20dp"
            android:textColor="#ffffff"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            android:layout_weight=".9"
            android:text="20/12/2019"
            android:textColor="#ffffff"
            android:textSize="20dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Inprogress"
            android:layout_weight=".9"
            android:textSize="20dp"
            android:textColor="#ffffff"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Pending"
            android:layout_weight=".9"
            android:textSize="20dp"
            android:textColor="#ffffff"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            ></TextView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"

        android:layout_height="wrap_content"
        android:gravity="center_horizontal">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:textColor="#ffffff"
            android:layout_marginTop="40dp"
            android:layout_weight=".9"
            android:text="12300"
            android:textSize="20dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="abcdd"
            android:layout_weight=".9"
            android:textSize="20dp"
            android:textColor="#ffffff"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ssssshid wed"
            android:layout_weight=".9"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            android:layout_weight=".9"
            android:text="20/12/2019"
            android:textColor="#ffffff"
            android:textSize="20dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Completed"
            android:layout_weight=".9"
            android:textSize="20dp"
            android:textColor="#ffffff"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            ></TextView>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Check now"
            android:layout_weight=".9"
            android:textSize="20dp"
            android:textColor="#ffffff"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="40dp"
            ></TextView>
    </LinearLayout>
    </LinearLayout>




</HorizontalScrollView>
</LinearLayout>